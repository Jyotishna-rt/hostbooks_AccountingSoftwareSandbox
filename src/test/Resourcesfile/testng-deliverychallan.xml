<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Delivery Challan Module Suite">
    <test name="Delivery Challan Test Cases" preserve-order="true">
        <groups>
            <run>
                <include name="login"/>
                <include name = "deliverychallan"/>
            </run>
        </groups>

        <classes>

            <!-- Common Login -->
            <class name="testCases.loginTestCase">
                <methods>
                    <include name="login"/>
                    <include name="DashboardList"/>
                </methods>
            </class>

            <class name="testCases.zenoopen">
                <methods>
                    <include name="openlinks"/>
                    <include name="openDeliveryChallan"/>
                </methods>
            </class>

            <!-- Delivery Challan Module -->
            <class name="testCases.deliveryChallanTestCase.deliveryChallanAddTestCase">
                <methods>
                    <include name="getdeliveryadd"/>
                </methods>
            </class>

            <class name="testCases.deliveryChallanTestCase.deliveryChallanEditTestCase">
                <methods>
                    <include name="getdeliveryedit"/>
                </methods>
            </class>

            <class name="testCases.deliveryChallanTestCase.deliveryChallanCopyTestCase">
                <methods>
                    <include name="getdeliverycopy"/>
                </methods>
            </class>

            <class name="testCases.deliveryChallanTestCase.deliveryChallanDeleteTestCase">
                <methods>
                    <include name="getdeliverydelete"/>
                </methods>
            </class>

        </classes>
    </test>
</suite>